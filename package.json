{
  "name": "@my-workspace/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "nx run-many --target=serve --projects=client,server --parallel",
    "dev:client": "nx serve client",
    "dev:server": "nx serve server",
    "build": "nx run-many --target=build --projects=client,server",
    "build:client": "nx build client",
    "build:server": "nx build server",
    "start": "nx run-many --target=start --projects=server",
    "start:client": "nx preview client",
    "start:server": "nx start server",
    "lint": "nx run-many --target=lint --projects=client,server",
    "lint:client": "nx lint client",
    "lint:server": "nx lint server",
    "lint:fix": "nx run-many --target=lint --projects=client,server -- --fix",
    "test": "nx run-many --target=test --projects=client,server",
    "test:client": "nx test client",
    "test:server": "nx test server",
    "test:watch": "nx run-many --target=test --projects=client,server -- --watch",
    "test:coverage": "nx run-many --target=test --projects=client,server -- --coverage",
    "db:generate": "cd apps/server && npx drizzle-kit generate",
    "db:migrate": "cd apps/server && npx drizzle-kit migrate",
    "db:studio": "cd apps/server && npx drizzle-kit studio",
    "prepare": "husky",
    "shadcn:add": "cd apps/client && npx shadcn@latest add"
  },
  "private": true,
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
